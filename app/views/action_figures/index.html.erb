<% @action_figures.each do |action_figure| %>
  <h2>Name: <a href="/action_figures/<%= action_figure.id %>" ><%= action_figure.name %> </a></h2>
  <h2>Description: <%= action_figure.description %> </h2>
  <h2>Price: $<%= action_figure.price %> </h2>
  <img src= <%= action_figure.image %> >
  <br>
<% end %>